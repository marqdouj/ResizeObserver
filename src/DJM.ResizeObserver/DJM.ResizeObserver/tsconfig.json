{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": false,
    "target": "ES6",
    "outDir": "wwwroot/ts/js",
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "ts/**/*"
  ]
}
